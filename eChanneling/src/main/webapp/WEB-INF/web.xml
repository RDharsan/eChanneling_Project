<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="htttp://xmlns.jcp.org/xml/ns/javaee
		http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
		version="3.1">
		
		<servlet>
			<servlet-name>Dlogin</servlet-name>
			<servlet-class>com.DloginServlet</servlet-class>
		</servlet>
		
		<servlet-mapping>
			<servlet-name>Dlogin</servlet-name>
			<url-pattern>/log</url-pattern>
		</servlet-mapping>
		
		<servlet>
			<servlet-name>Doctor Registration</servlet-name>
			<servlet-class>com.DregServlet</servlet-class>
		</servlet>
		<servlet-mapping>
		<servlet-name>Doctor Registration</servlet-name>
		<url-pattern>/reg</url-pattern>
		</servlet-mapping>
		
		
		<servlet>
			<servlet-name>updatedoctor</servlet-name>
			<servlet-class>com.updateDoctorServlet</servlet-class>
		</servlet>
		
		<servlet-mapping>
			<servlet-name>updatedoctor</servlet-name>
			<url-pattern>/update</url-pattern>
		</servlet-mapping>
		
		<servlet>
			<servlet-name>enterps</servlet-name>
			<servlet-class>com.enterpidServlet</servlet-class>
		</servlet>
		
		
		<servlet-mapping>
			<servlet-name>enterps</servlet-name>
			<url-pattern>/enter</url-pattern>
		</servlet-mapping>

		<servlet>
			<servlet-name>addpres</servlet-name>
			<servlet-class>com.prescriptionServet</servlet-class>
		</servlet>
		
		<servlet-mapping>
			<servlet-name>addpres</servlet-name>
			<url-pattern>/add</url-pattern>
		</servlet-mapping>
		
		<servlet>
			<servlet-name>viewpres</servlet-name>
			<servlet-class>com.ViewprescriptionServlet</servlet-class>
		</servlet>
		
		<servlet-mapping>
			<servlet-name>viewpres</servlet-name>
			<url-pattern>/view</url-pattern>
		</servlet-mapping>
		
		<servlet>
			<servlet-name>updatePres</servlet-name>
			<servlet-class>com.updatePrescriptionServlet</servlet-class>
		</servlet>
		
		<servlet-mapping>
			<servlet-name>updatePres</servlet-name>
			<url-pattern>/updatepres</url-pattern>
		</servlet-mapping>
		
		<servlet>
			<servlet-name>deletepres</servlet-name>
			<servlet-class>com.deletePrescriptionServlet</servlet-class>
		</servlet>
		
		<servlet-mapping>
			<servlet-name>deletepres</servlet-name>
			<url-pattern>/delete</url-pattern>
		</servlet-mapping>
		
		
</web-app>